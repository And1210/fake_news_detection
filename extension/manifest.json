{
  "manifest_version": 2,
  "name": "Fake News Detection",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.5.1.min.js", "content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
      "default_popup": "popup.html"
  },
  "permissions": [
      "tabs","http://*/*","https://*/*"
  ]
}
